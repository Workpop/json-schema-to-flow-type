{
  "name": "@workpop/json-schema-to-flow-type",
  "version": "0.0.1",
  "ava": {
    "files": [
      "**/__tests__/*.spec.js"
    ],
    "require": [
      "babel-register"
    ],
    "failFast": true,
    "babel": "inherit"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "npm run lint && ava -v",
    "build": "rm -rf lib && babel src --ignore __**__ -d lib",
    "prepublish": "flow-typed update && npm test && npm run build",
    "release": "standard-version"
  },
  "devDependencies": {
    "@workpop/eslint-config-workpop": "^0.0.8",
    "ava": "0.17.x",
    "babel-cli": "6.18.x",
    "babel-core": "6.18.x",
    "babel-generator": "6.19.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.22.0",
    "eslint": "3.10.x",
    "flow-bin": "0.35.x",
    "flow-typed": "2.0.x",
    "fs-extra": "1.0.0",
    "standard-version": "^4.0.0"
  },
  "dependencies": {
    "babel-types": "6.19.0",
    "lodash": "4.17.x"
  },
  "main": "lib/index.js",
  "files": [
    "lib/**"
  ],
  "license": "MIT",
  "repository": {
    "url": "git@github.com:Workpop/json-schema-to-flow-type.git",
    "type": "git"
  },
